{
  "name": "bin381-data-analysis-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --names \"NEXT,TASK-API,ML-API\" --prefix-colors \"cyan,magenta,yellow\" \"npm run next-dev\" \"npm run flask-task\" \"npm run flask-project\"",
    "next-dev": "next dev",
    "flask-task": "cd \"01_TaskManagement/api\" && set PYTHONIOENCODING=utf-8 && venv\\Scripts\\python.exe app.py",
    "flask-project": "cd \"02_Project/api\" && set PYTHONIOENCODING=utf-8 && venv\\Scripts\\python.exe app.py",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "css-watch": "npx @tailwindcss/cli -i app/input.css -o app/globals.css --watch",
    "css-build": "npx @tailwindcss/cli -i app/input.css -o app/globals.css --minify"
  },
  "dependencies": {
    "next": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^8.2.2",
    "tailwindcss": "^4.1.1",
    "@tailwindcss/postcss": "^4.1.1",
    "@tailwindcss/cli": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.5.6"
  }
}